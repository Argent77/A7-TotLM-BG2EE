IF
  !Global("EFXDELL_inactive","LOCALS",1)
  OR(2)
    LevelPartyLT(12)
    DifficultyLT(NORMAL)
THEN
  RESPONSE #100
    IncrementGlobal("SPRITE_IS_DEADSHAMBLER","GLOBAL",1)
    DestroySelf()
END

IF
  !Global("EFXDELL_inactive","LOCALS",1)
  True()
THEN
  RESPONSE #100
    SetGlobal("EFXDELL_inactive","LOCALS",1)
END


IF
  !Global("DOOR_6_UNLOCKED","MYAREA",0)
THEN
  RESPONSE #100
    Deactivate(Myself)
END

IF
  Global("DOOR_6_UNLOCKED","MYAREA",0)
  OR(2)
    Unlocked("ID9714_DOOR6")
    OpenState(Myself,TRUE)
THEN
  RESPONSE #100
    OpenDoor("ID9714_Plate6")
    SetGlobal("DOOR_6_UNLOCKED","MYAREA",1)
    TakePartyItemAll("KeyBron")
END


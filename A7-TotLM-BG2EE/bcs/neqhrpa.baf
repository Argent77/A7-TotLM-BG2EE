IF
  OR(2)
    LevelPartyGT(14)
    DifficultyGT(NORMAL)
THEN
  RESPONSE #100
    ChangeAIScript("NEQHRPAH",CLASS)
END

IF
  OR(2)
    LevelPartyLT(12)
    DifficultyLT(EASY)
THEN
  RESPONSE #100
    ChangeAIScript("NEQHRPAL",CLASS)
END

IF
  True()
THEN
  RESPONSE #100
    ChangeAIScript("NEQHRPAM",CLASS)
END


IF
  !Global("DOOR_1_UNLOCKED","MYAREA",0)
THEN
  RESPONSE #100
    Deactivate(Myself)
END

IF
  Global("DOOR_1_UNLOCKED","MYAREA",0)
  OR(2)
    Unlocked("ID9714_DOOR1")
    OpenState(Myself,TRUE)
THEN
  RESPONSE #100
    StaticSequence("9714MS3",1) // SEQ_AWAKE
    SetGlobal("DOOR_1_UNLOCKED","MYAREA",1)
    TakePartyItemAll("KeyPlat")
END


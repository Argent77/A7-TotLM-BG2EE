IF
  !Global("TALE_PLACED","MYAREA",0)
  !Contains("TALE1",Myself)
  Contains("blun01",Myself)
  OR(2)
    Entered([PC])
    Opened([PC])
THEN
  RESPONSE #100
    DestroyItem("blun01")
    DisplayString(Myself,14381)
    ReallyForceSpell(LastTrigger,TRAP_LIGHTNING_BOLT)
    Wait(2)
  RESPONSE #100
    DestroyItem("blun01")
    DisplayString(Myself,14381)
    ReallyForceSpell(LastTrigger,TRAP_FIREBALL)
    Wait(2)
END


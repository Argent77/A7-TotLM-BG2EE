IF
  !Global("DOOR_5_UNLOCKED","MYAREA",0)
THEN
  RESPONSE #100
    Deactivate(Myself)
END

IF
  Global("DOOR_5_UNLOCKED","MYAREA",0)
  OR(2)
    Unlocked("ID9714_DOOR5")
    OpenState(Myself,TRUE)
THEN
  RESPONSE #100
    OpenDoor("ID9714_Plate5")
    SetGlobal("DOOR_5_UNLOCKED","MYAREA",1)
    TakePartyItemAll("KeyCopp")
END

